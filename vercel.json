{
    "version": 2,
    "builds": [
      {
        "src": "user/*.php",
        "use": "@vercel/php"
      },
      {
        "src": "produk/*.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/user/list",
        "dest": "/user/list.php"
      },
      {
        "src": "/produk/list_produk",
        "dest": "/produk/list_produk.php"
      }
    ]
  }
  